<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.wuyunxing.tuacydemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NavigationViewActivity"/>
        <activity android:name=".NavigationViewDownActivity"/>
        <activity android:name=".cardview.CardViewActivity"/>
        <activity android:name=".materialdesignanimation.DesignAnimation"/>
        <activity android:name=".ItemTouchHelper.ItemTouchHelperActivity"/>
    </application>

</manifest>
